{
  "version": 2,
  "builds": [
    {
      "src": "server-vercel.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "server-vercel.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "server-vercel.js"
    },
    {
      "src": "/(.*)",
      "dest": "server-vercel.js"
    }
  ],
  "functions": {
    "server-vercel.js": {
      "maxDuration": 30
    }
  }
} 
